{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ES2020",
        "moduleResolution": "node",
        "checkJs": true,
        "allowJs": true,
        "skipLibCheck": true
    },
    "include": [
        "js/**/*",
        "*.js",
        "types/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}